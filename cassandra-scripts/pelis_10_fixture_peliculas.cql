INSERT INTO peliculas (id, fecha_estreno, titulo, sinopsis) values (uuid(), '2006-07-01', 'Les Luthiers : Grandes Hitos Antologia (1995)','');
INSERT INTO peliculas (id, fecha_estreno, titulo, sinopsis) values (uuid(), '2006-07-01', 'Les Luthiers : Hacen mucha gracia de nada (1980)','');
